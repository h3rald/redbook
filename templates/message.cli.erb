<%
	prefix = ">>"
	message = @message.value
	case @message.name
	when :info
		prefix = prefix.green
		message = message.dark_green
	when :warning
		prefix = prefix.yellow
		message = message.dark_yellow
	when :error
		prefix = prefix.red
		message = message.dark_red
	when :debug
		prefix = prefix.magenta
		message = message.dark_magenta
	end	
%> <%= "#{prefix} #{message}" -%>
