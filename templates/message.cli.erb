<%
	name = ""
	message = @message.value
	case @message.name
	when :info
		name = "[i]".green
		message = message.dark_green
	when :warning
		name = "[w]".yellow
		message = message.dark_yellow
	when :error
		name = "[e]".red
		message = message.dark_red
	when :debug
		name = "[d]".magenta
		message = message.dark_magenta
	end	
%> <%= "#{name} #{message}" -%>
